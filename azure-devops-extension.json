{
  "manifestVersion": 1,
  "id": "repository-advanced-creator",
  "publisher": "JasonWhalley",
  "version": "0.0.1",
  "name": "Repository Advanced Create",
  "description": "Create a Repository with supplemental groups",
  "categories": [
    "Azure Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "logo.png"
  },
  "contributions": [
    {
      "id": "repository-advanced-creator-action",
      "type": "ms.vss-web.menu-item",
      "targets": [
        "ms.vss-code-web.command-bar-repository-picker-actions"
      ],
      "properties": {
        "text": "Advanced Repository Creation",
        "uri": "dist/index.html",
        "iconName": "Add",
        "registeredObjectId": "repository-advanced-creation"
      }
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    }
  ]
}