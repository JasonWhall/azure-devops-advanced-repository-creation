{
  "$schema": "https://json.schemastore.org/vss-extension.json",
  "manifestVersion": 1,
  "id": "repository-advanced-creator",
  "publisher": "JasonWhalley",
  "version": "0.1.3",
  "name": "Advanced Repository Creator",
  "description": "Create a Repository with supplemental groups",
  "categories": [
    "Azure Repos"
  ],
  "tags": [
    "Azure DevOps Extensions",
    "Repository",
    "Create",
    "Groups"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "img/logo.png"
  },
  "badges": [
    {
      "href": "https://github.com/JasonWhall/azure-devops-advanced-repository-creation",
      "uri": "https://github.com/JasonWhall/azure-devops-advanced-repository-creation/actions/workflows/build.yaml/badge.svg",
      "description": "Node CI"
    }
  ],
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/JasonWhall/azure-devops-advanced-repository-creation"
  },
  "screenshots": [
    {
      "path": "img/panel-example.png"
    }
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "public": true
}
