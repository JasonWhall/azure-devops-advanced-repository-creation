{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": [
    "src"
  ]
}