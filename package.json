{
  "name": "azure-devops-repository-advanced",
  "version": "0.0.1",
  "description": "Azure DevOps Extension to Create a Repository with supplemental groups",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run compile",
    "dev": "webpack-dev-server --config webpack.dev.config.js --static ./dist",
    "compile": "npm run clean && webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "tfx extension create --manifest-globs azure-devops-extension.json",
    "package:dev": "tfx extension create --manifest-globs azure-devops-extension.json --overrides-file azure-devops-extension-dev.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonWhall/azure-devops-repository-advanced.git"
  },
  "keywords": [
    "Azure",
    "DevOps"
  ],
  "author": "JasonWhall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonWhall/azure-devops-repository-advanced/issues"
  },
  "homepage": "https://github.com/JasonWhall/azure-devops-repository-advanced#readme",
  "dependencies": {
    "azure-devops-extension-api": "^1.158.0",
    "azure-devops-extension-sdk": "^2.0.11",
    "azure-devops-ui": "^2.167.52",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/q": "^1.5.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "loader-utils": "^3.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tfx-cli": "^0.12.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.1.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
